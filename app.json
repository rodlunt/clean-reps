{
  "expo": {
    "name": "Clean Reps",
    "slug": "clean-reps",
    "version": "1.0.6",
    "orientation": "default",
    "icon": "./assets/logo-light.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#14B8A6"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.cleanreps.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#14B8A6"
      },
      "package": "com.cleanreps.app"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "kotlinVersion": "2.0.21",
            "targetSdkVersion": 35,
            "compileSdkVersion": 35,
            "useLegacyPackaging": false,
            "enableProguardInReleaseBuilds": true,
            "extraProguardRules": "-keep class com.cleanreps.** { *; }"
          }
        }
      ],
      [
        "expo-navigation-bar",
        {
          "position": "absolute",
          "visibility": "visible",
          "behavior": "overlay-swipe"
        }
      ],
      "react-native-edge-to-edge"
    ],
    "androidStatusBar": {
      "barStyle": "light-content",
      "translucent": true
    },
    "androidNavigationBar": {
      "visible": "sticky-immersive",
      "barStyle": "light-content"
    },
    "extra": {
      "eas": {
        "projectId": "aaa2b1ad-8339-4b3b-819c-023ae6fe85f8"
      }
    }
  }
}
